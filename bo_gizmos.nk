addUserKnob {22 setCenter l "Set Center" -STARTLINE T "this_width = nuke.thisNode().width()\n\nthis_height = nuke.thisNode().height()\n\nnewWidth = this_width/2\nnewHeight = this_height/2\n\nnuke.thisNode().knob(\"center\").setValue(\[newWidth, newHeight])"}
addUserKnob {41 invert_matrix l invert T Transform1_ANIM1.invert_matrix}
addUserKnob {41 filter T Transform1_ANIM1.filter}
