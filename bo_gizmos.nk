 addUserKnob {20 User}
 addUserKnob {26 waveanim l "<h3><font color=#e665eb>Wave Anim"}
 addUserKnob {41 translate_1 l Center T Transform1.translate}
 addUserKnob {22 setcenter l "Set Center" -STARTLINE T "this_width = nuke.thisNode().width()\n\nthis_height = nuke.thisNode().height()\n\nnewWidth = this_width/2\nnewHeight = this_height/2\n\nnuke.thisNode().knob(\"translate_1\").setValue(\[newWidth, newHeight])"}
 addUserKnob {41 x_offset l "X Offset" +INVISIBLE T Transform_proxy.x_offset}
 addUserKnob {41 y_offset l "Y Offset" +INVISIBLE T Transform_proxy.y_offset}
 addUserKnob {41 wavelength l "Wave Length" T HSVTool_Wave_MAIN.wavelength}
 addUserKnob {41 offset l "Wave Offset" T PROXY_OFFSET.offset}
 addUserKnob {26 wave_size l "<h3><font color=#e665eb>Wave Size"}
 addUserKnob {41 size_offset l "Wave Size Offset" T PROXY_OFFSET.size_offset}
 addUserKnob {22 halfStripes l "<font color=#888888>Half Stripes OFF" T "## HALF STRIPES ###\ncWave = nuke.thisGroup()\nknobThis = nuke.thisKnob()\n\n# entering the Group\ncWave.begin()\n\n# toggle disabling every second Keymix via proxy knob\nknob1 = nuke.toNode('PROXY_OFFSET')\['half_stripes'].value()\nif knob1 == True:\n    nuke.toNode('PROXY_OFFSET')\['half_stripes'].setValue(False)\nelse:\n    nuke.toNode('PROXY_OFFSET')\['half_stripes'].setValue(True)\n\n# toggle values in every HSVTools\nnodes = nuke.allNodes('HSVTool')\n\nif nuke.toNode('HSVTool_WaveTEST')\['offset'].animation(0).expression()\[0:3] == \"20.\":\n    for node in nodes:\n        try:\n            currOffset = node\['offset'].animation(0).expression().split('+')\[0]\n            node\['offset'].setExpression(str(float(currOffset) / 2) + \" + PROXY_OFFSET.offset\")\n        except Exception:\n            pass\nelse:\n    for node in nodes:\n        try:\n            currOffset = node\['offset'].animation(0).expression().split('+')\[0]\n            node\['offset'].setExpression(str(float(currOffset) * 2) + \" + PROXY_OFFSET.offset\")\n        except Exception:\n            pass\n\n# leaving the Group\ncWave.end()\n\n\n# toggle between ON/OFF labels\ndef toggleLabel():\n    options = \[\"\"\"<font color=#dddddd>Half Stripes ON\"\"\", \"<font color=#888888>Half Stripes OFF\"]\n    knobThis.setLabel(options\[int(knobThis.label() == options\[0])])\ntoggleLabel()\n" +STARTLINE}
 addUserKnob {41 size l "Blur size" T Blur_MAIN.size}
 addUserKnob {26 wavegrade l "<h3><font color=#e665eb>Wave Grade"}
 addUserKnob {41 multiply l Brightness T Grade1.multiply}
 addUserKnob {41 saturation l Saturation T Saturation1.saturation}
 addUserKnob {26 ""}
 addUserKnob {20 offset_center l "Offset Center" n 1}
 offset_center 0
 addUserKnob {26 instr l "" +STARTLINE T " \n          This part is created for cases when you\n          want to animate the center with linking \n          to an existing animation curve. \n          Instructions:\n              1. Do the linking\n              2. Click 'Enable Offset'\n              3. You are good to use the sliders!"}
 addUserKnob {26 empty l "" +STARTLINE T " "}
 addUserKnob {22 enable_offset l "<font color=#cccccc>Enable Offset" T "#Enable Offset\ncWave = nuke.thisGroup()\ncWave.begin()\n\ntrans1 = nuke.toNode('Transform1')\nif trans1\['translate'].hasExpression():\n    x_val = trans1\['translate'].animation(0).expression()\n    y_val = trans1\['translate'].animation(1).expression()\n\n    trans1\['translate'].setExpression(x_val + \" + Transform1.x_offset\", 0)\n    trans1\['translate'].setExpression(y_val + \" + Transform1.y_offset\", 1)\n\n    cWave.end()\n    def toggleLabel():\n        knob1 = cWave\['enable_offset']\n        options = \[\"\"\"<font color=#20bd2b>Offset Enabled\"\"\", \"<font color=#cccccc>Enable Offset\"]\n        knob1.setLabel(options\[int(knob1.label() == options\[0])])\n\n        knob2 = cWave\['rem_anim']\n        options2 = \[\"\"\"<font color=#e38764>Remove Expression\"\"\", \"<font color=#cccccc>No Expression\"]\n        knob2.setLabel(options2\[int(knob2.label() == options2\[0])])\n\n    toggleLabel()\n\nelse:\n    nuke.message(\"\"\"<center><b><font color=orange>The value has no expression.\\nTherefore no need to offset!\\n\\n<a href=\"https://learn.foundry.com/nuke/8.0/content/user_guide/expressions/linking_expressions.html\"><font color=yellow><u>Learn how to link animation!\\n\"\"\")\n\ncWave.end()" +STARTLINE}
 addUserKnob {22 rem_anim l "<font color=#cccccc>No Expression" -STARTLINE T "#Remove Animation\ncWave = nuke.thisGroup()\ncWave.begin()\n\ntrans1 = nuke.toNode('Transform1')\nif trans1\['translate'].hasExpression():\n    trans1\['translate'].clearAnimated()\n    cWave.end()\n    def toggleLabel():\n        knob1 = cWave\['enable_offset']\n        options = \[\"\"\"<font color=#20bd2b>Offset Enabled\"\"\", \"<font color=#cccccc>Enable Offset\"]\n        knob1.setLabel(options\[int(knob1.label() == options\[0])])\n\n        knob2 = cWave\['rem_anim']\n        options2 = \[\"\"\"<font color=#e38764>Remove Expression\"\"\", \"<font color=#cccccc>No Expression\"]\n        knob2.setLabel(options2\[int(knob2.label() == options2\[0])])\n\n    toggleLabel()\nelse:\n    nuke.message(\"\"\"<center><b><font color=orange>There is no expression to be removed\"\"\")\n\ncWave.end()"}
 addUserKnob {41 x_offset_1 l "X Offset" T Transform1.x_offset}
 addUserKnob {41 y_offset_1 l "Y Offset" T Transform1.y_offset}
 addUserKnob {20 endGroup n -1}
 addUserKnob {26 ""}
 addUserKnob {41 which l Mix T Dissolve32.which}
 addUserKnob {20 info l Info}
 addUserKnob {26 info1 l "" +STARTLINE T "<h1><font color=#888888>1.01 version</h1>GM_ColorWave<font color=#777777> by Attil Gasparetz\n\n2020"}
