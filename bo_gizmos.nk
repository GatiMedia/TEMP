}
Ramp {
output {-rgba.red rgba.green -rgba.blue -rgba.alpha}
p0 {0 0}
p1 {0 {parent.Crop2.box.t}}
name Ramp_GREEN
label "in Y"
xpos 620
ypos 560
}
Ramp {
output {rgba.red -rgba.green -rgba.blue -rgba.alpha}
p0 {0 0}
p1 {{parent.Crop2.box.r} 0}
name Ramp_RED
label "in X"
xpos 620
ypos 608
}
Expression {
temp_name0 offset
temp_expr0 (1-STmap_Ctrl.scale)/(2*STmap_Ctrl.scale)
temp_name1 gain
temp_expr1 STmap_Ctrl.scale
expr0 (red+offset)*gain
expr1 (green+offset)*gain
expr2 0
name Expression1
selected true
xpos 620
ypos 662
}
Reformat {
format {{{parent.input.format}}}
resize none
pbb true
name Reformat1
xpos 620
ypos 710
}
Output {
name Output1
xpos 620
ypos 758
}
NoOp {
inputs 0
name STmap_Ctrl
xpos 400
ypos 518
addUserKnob {20 User}
addUserKnob {7 scale R 1 10}
scale 1.45
}
end_group
